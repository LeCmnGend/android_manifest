<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="lecmngend"
           fetch="https://github.com/LeCmnGend"
           sync-c="true"
		   revision="refs/heads/cr-R" />

  <!-- Build Repos -->
  <project path="build/make" name="android_build_make" remote="lecmngend" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>

  <project path="build/soong" name="android_build_soong" remote="lecmngend" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>

  <!-- Frameworks Repos -->
  <project path="frameworks/native" name="android_frameworks_native" remote="lecmngend" />

  <!-- Prebuilts Repos -->
  <project path="prebuilts/abi-dumps/vndk" name="android_prebuilts_abi-dumps_vndk" remote="lecmngend" />
  <project path="vendor/qcom/opensource/commonsys/system/bt" name="android_vendor_qcom_opensource_system_bt" remote="lecmngend" />

</manifest>
