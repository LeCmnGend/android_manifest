<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="lecmngend"
           fetch="https://github.com/LeCmnGend"
           sync-c="true"
		   revision="refs/heads/super-R" />

  <project path="art" name="android_art" groups="pdk" remote="lecmngend"/>
